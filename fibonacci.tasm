.data
trytes:
	%tryte 1
	%tryte 2
	%tryte 3

string:
	%string "Hello, world!"

.code
start:
	movi $a0, 0 ; a
	movi $a1, 1 ; b
	movi $s0, 0 ; i
	movi $s1, 8 ; iterations

loop__entry:
	cmp $t0, $s0, $s1
	j01 $t0, loop__exit

loop__body:
	; (a, b) = (b, a + b)
	add $t0, $a0, $a1
	mov $a0, $a1
	mov $a1, $t0

	; continue loop
	addi $s0, 1
	jmp loop__entry

loop__exit:
	halt
